{
  "name": "@morpheusnephew/td-ameritrade",
  "version": "0.4.9",
  "description": "A client for TD Ameritrade",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MorpheusNephew/TdAmeritrade.git"
  },
  "scripts": {
    "test": "jest --verbose",
    "start": "ts-node ./src/index.ts",
    "build": "rimraf ./dist && tsc",
    "publishToNpm": "yarn run build && yarn publish --access public",
    "postpublishToNpm": "yarn pushTags && yarn updateChangeLog",
    "pushTags": "git push --tags",
    "updateChangeLog": "git-chglog -o CHANGELOG.md",
    "prepare": "husky install"
  },
  "author": "Jelani Jackson",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.11.8",
    "@types/qs": "^6.9.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": ">=6",
    "jest": "^27.2.0",
    "jest-ts-auto-mock": "^1.0.11",
    "lint-staged": ">=10",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-auto-mock": "^2.6.5",
    "ts-jest": "^27.0.1",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@morpheusnephew/td-ameritrade-models": "^0.12.9",
    "axios": "^0.21.1",
    "qs": "^6.9.4"
  },
  "keywords": [
    "TdAmeritrade",
    "TD",
    "Ameritrade"
  ],
  "bugs": {
    "url": "https://github.com/MorpheusNephew/TdAmeritrade/issues"
  },
  "homepage": "https://github.com/MorpheusNephew/TdAmeritrade#readme",
  "directories": {
    "test": "tests"
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write"
  }
}
